    "react-redux": "^4.0.0",
    "react-router": "^2.0.0",
    "react-router-redux": "^4.0.0-beta",
    "redux": "^3.0.0",
    "redux-thunk": "^2.1.0",
    "url": "^0.11.0"
  },
